{
  "name": "baidu-netdisk-sdk",
  "version": "0.1.1",
  "description": "BaiduNetdiskSdk based on BaiduNetdiskApi",
  "keywords": [
    "baidu",
    "netdisk",
    "sdk"
  ],
  "author": "keenghost",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keenghost/BaiduNetdiskNode.git",
    "directory": "packages/sdk"
  },
  "type": "module",
  "main": "./index.ts",
  "dependencies": {
    "baidu-netdisk-api": "workspace:*"
  },
  "engines": {
    "node": "^18.0.0 || >= 20.0.0",
    "pnpm": "^9.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "test": "tsx test/index.test.ts",
    "build": "tsx scripts/build.ts",
    "release": "cd dist && npm publish"
  },
  "moduleOptions": {
    "type": "module",
    "main": "./index.js",
    "module": "./index.js",
    "types": "./index.d.ts",
    "files": [
      "src",
      "index.d.ts",
      "index.js",
      "index.cjs"
    ],
    "exports": {
      ".": {
        "types": "./index.d.ts",
        "import": "./index.js",
        "require": "./index.cjs"
      }
    }
  }
}
